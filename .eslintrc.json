{
  "extends": "airbnb",
  "globals": {
    "$": true,
    "require": true,
    "define": true,
    "requirejs": true,
    "beforeAll": true,
    "it": true,
    "describe": true,
    "expect": true,
    "done": true,
    "fail": true,
    "app": true
  },
  "rules": {
    "comma-dangle": "off",
    "func-names": "off",
    "space-before-function-paren": "off",
    "new-cap": "off",
    "max-len": "off",
    "quote-props": "off",
    "import/no-amd": "off",
    "global-require": "off"
  },
  "plugins": [
    "react"
  ]
}
